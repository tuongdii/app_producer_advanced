               <property name="styleSheet">
                <string notr="true">/* Thiết lập CSS cho combobox */
QComboBox {
    color: #575556;
    font: 14pt &quot;Bahnschrift&quot;;
    border-style: solid;
    border-width: 2px;
    border-color: #ECE7E0;
    padding-left: 10px;
    border-radius: 15px;
    background-color: rgba(255, 0, 0, 0); /* Màu nền sẽ không ảnh hưởng do transparency (alpha = 0) */
}

/* Thiết lập màu nền cho các mục trong combobox khi chưa hover */
QComboBox::item {
    background-color: #575556;
	border-radius: 0px;
}

/* Thiết lập màu nền cho các mục trong combobox khi hover */
QComboBox::item:hover {
    background-color: #ECE7E0;
	border-radius: 0px;
}

QComboBox::item:selected {
    background-color: #ECE7E0;
	border-radius: 0px;
}
</string>
